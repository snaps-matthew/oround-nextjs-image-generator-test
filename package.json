    "react-redux": "^7.2.6",
    "tailwindcss": "^1.9.6"
  },
  "devDependencies": {
    "@types/node": "16.11.6",
    "@types/react": "17.0.33",
    "@types/react-redux": "^7.1.20",
    "autoprefixer": "^10.4.0",
